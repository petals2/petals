class Vector2 {
  protected readonly x: number;
  protected readonly y: number;

  constructor(x: number, y: number) {
    this.x = x;
    this.y = y;
  }

  add(v: &Vector2): void {
    this.x = this.x + v.x;
    this.y = this.y + v.y;
  }
}

var a = new Vector2(1, 2);
