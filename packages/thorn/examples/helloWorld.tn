class Vector2 {
  protected x: number;
  protected y: number;

  constructor(x: number, y: number) {
    this.x = x;
    this.y = y;
  }
}

function sumVectors(v: &list<Vector2>): Vector2 {
  var sum = new Vector2(0, 0)

  for (var i = 0; i < v.length; i++) {
    sum.add(v[i]);
  }

  return sum;
}

var s = sumVectors(&[
  new Vector2(1, 2),
  new Vector2(3, 4),
  new Vector2(5, 6),
])

var sx = s.x;
var sy = s.y;
